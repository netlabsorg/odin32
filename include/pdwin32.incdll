# ODIN Project
#
# Common DLL makefile rules
# Note: Must be included after the 'all' rule!
#

$(TARGET)exp.def: $(TARGET).def
    $(IMPDEF) $** $@

$(TARGET).lib: $(TARGET)exp.Def
    $(IMPLIB) $(IMPLIBFLAGS) $@ $**
    $(CP) $@ $(PDWIN32_LIB)\$@
