        GROUPBOX        "ELF Loader", GR_LDR_ELF, 142, 152, 118, 37
        AUTOCHECKBOX    "Enabled", CB_LDR_ELF_ENABLED, 150, 168, 55, 10
        LTEXT           "Infolevel:", TX_LDR_ELF_INFOLEVEL, 154, 157, 48, 8
        SPINBUTTON      SB_LDR_ELF_INFOLEVEL, 206, 157, 28, 12, SPBS_NUMERICONLY |
                        SPBS_MASTER | WS_GROUP
        AUTOCHECKBOX    "UNIX Shell Scripts", CB_LDR_SHELL_SCRIPTS, 14, 124,
                        160, 10
        AUTOCHECKBOX    "JAVA", CB_LDR_JAVA, 14, 114, 160, 10
        AUTOCHECKBOX    "(Pure) REXX Scripts (.RX/.REX)", CB_LDR_REXX, 14,
                        104, 160, 10
        GROUPBOX        "Heaps", GR_HEAPS, 6, 26, 262, 65
        GROUPBOX        "Resident Heap", GR_HEAP_RES, 14, 33, 118, 48
        LTEXT           "Max (KB):", TX_HEAP_RES_MAX, 22, 62, 53, 8
        SPINBUTTON      SB_HEAP_RES_MAX, 80, 62, 42, 8, SPBS_NUMERICONLY |
                        SPBS_MASTER | WS_GROUP | SPBS_FASTSPIN

        LTEXT           "Initial (KB)", TX_HEAP_RES_INIT, 22, 50, 50, 8
        RTEXT           "256", TX_HEAP_RES_INIT_VAL, 78, 50, 46, 8
        LTEXT           "Used (KB)", TX_HEAP_RES_USED, 22, 38, 50, 8
        RTEXT           "256", TX_HEAP_RES_USED_VAL, 78, 38, 46, 8
        GROUPBOX        "Swappable Heap", GR_HEAP_SWP, 142, 33, 118, 48
        LTEXT           "Max (KB):", TX_HEAP_SWP_MAX, 150, 62, 53, 8
        SPINBUTTON      SB_HEAP_SWP_MAX, 208, 62, 42, 8, SPBS_NUMERICONLY |
                        SPBS_MASTER | WS_GROUP | SPBS_FASTSPIN
        LTEXT           "Initial (KB)", TX_HEAP_SWP_INIT, 150, 50, 50, 8
        RTEXT           "256", TX_HEAP_SWP_INIT_VAL, 206, 50, 46, 8
        LTEXT           "Used (KB)", TX_HEAP_SWP_USED, 150, 38, 50, 8
        RTEXT           "256", TX_HEAP_SWP_USED_VAL, 206, 38, 46, 8
        DEFPUSHBUTTON   "~Refresh", PB_REFRESH, 6, 4, 40, 14
        PUSHBUTTON      "Apply", PB_APPLY, 49, 4, 40, 14
        PUSHBUTTON      "Close", DID_OK, 92, 4, 40, 14
