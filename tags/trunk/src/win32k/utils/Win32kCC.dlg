#ifndef OS2_INCLUDED
   #include <os2.h>
   #include "win32kcc.h"
#endif
DLGINCLUDE 1 "win32kcc.h"

DLGTEMPLATE DL_WIN32KCC LOADONCALL MOVEABLE DISCARDABLE
BEGIN
    DIALOG  "Win32k Control Center", DL_WIN32KCC, 173, 69, 274, 256, WS_VISIBLE, FCF_SYSMENU |
            FCF_TITLEBAR | FCF_TASKLIST
    BEGIN
        AUTOCHECKBOX    "Enabled", CB_LOGGING_ENABLED, 14, 228, 50, 10
        GROUPBOX        "Logging", GR_LOGGING, 6, 225, 216, 25
        AUTORADIOBUTTON "COM1", RB_LOGGING_COM1, 68, 228, 36, 10, WS_TABSTOP
        AUTORADIOBUTTON "COM2", RB_LOGGING_COM2, 106, 228, 36, 10,
                        WS_TABSTOP
        AUTORADIOBUTTON "COM3", RB_LOGGING_COM3, 142, 228, 36, 10,
                        WS_TABSTOP
        AUTORADIOBUTTON "COM4", RB_LOGGING_COM4, 180, 228, 36, 10,
                        WS_TABSTOP
        GROUPBOX        "Loaders", GR_LOADERS, 6, 100, 262, 118
        AUTOCHECKBOX    "Disable all loaders", CB_LDR_DISABLE_ALL, 14, 196,
                        112, 10
        GROUPBOX        "PE Loader", 101, 14, 142, 118, 48
        AUTORADIOBUTTON "Pure", RB_LDR_PE_PURE, 22, 170, 44, 10, WS_TABSTOP
        AUTORADIOBUTTON "Mixed", RB_LDR_PE_MIXED, 22, 160, 44, 10,
                        WS_TABSTOP
